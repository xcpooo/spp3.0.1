<?xml version="1.0" encoding="utf-8" ?>

<proxy groupid="0">

    <acceptor timeout="60" udpclose=”0”>

        <entry type="tcp" if="eth1" port="10085" TOS="123" />

        <entry type="udp" if="eth1" port="10085"/>

        <entry type="unix" path="/tmp/spp_proxy_unix"/>

    </acceptor>



    <connector>

        <entry send_size="16" recv_size="16" groupid = "1"/>

        <entry send_size="16" recv_size="16" groupid = "2"/>

    </connector>



    <flog level="3" type="0" path="../log" prefix="spp_frame_proxy" maxfilesize="-1" maxfilenum="10"/>

    <log level="3" type="0" path="../log" 

        prefix="spp_proxy" maxfilesize="10240000" maxfilenum="10"/>

    <fstat file="../stat/stat_spp_proxy.dat" intervial="30"/>

    <stat file="../stat/module_stat_spp_proxy.dat" intervial="30"/>

    <moni intervial="30" level="2" path="../moni" prefix="moni_spp_proxy" maxfilesize="10240000" maxfilenum="10"/>

    <module bin="./spp_module_test.so" etc=""/>

</proxy>



